	<queryString><![CDATA[SELECT PRODUCTS.ID, PRODUCTS.REFERENCE, PRODUCTS.IMAGE AS IMAGE, PRODUCTS.CODE AS CODE, PRODUCTS.NAME, PRODUCTS.PRICEBUY, PRODUCTS.PRICESELL, (PRODUCTS.PRICESELL * (1 + TAXES.RATE)) AS PRICESELLTAX, TAXES.NAME AS TAXNAME, TAXES.RATE AS TAXRATE, PRODUCTS.CATEGORY, CATEGORIES.NAME AS CATEGORYNAME
